{% extends "_base.console.twig" %}

{% block type %}<t_array>{{ parent() }}({{ var.length }})</t_array><space>{% endblock %}

{% block value %}
    <intro>
    {% if not var.terminal %}
        {% for item in var.value %}
            {{ m.indent(var.level + 1) }}
            {{ render_type(item.value, item.key) }}
            {% if not loop.last %}<intro>{% endif %}
        {% endfor %}
    {% else %}
        {{ m.indent(var.level) }}(!)<space>Array<space>max<space>nesting<space>level<space>reached<intro>
    {% endif %}
{% endblock %}
